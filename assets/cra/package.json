{
  "name": "timeline",
  "version": "0.0.0+12",
  "private": true,
  "description": "",
  "type": "module",
  "main": "src/index.jsx",
  "repository": "https://github.com/netlywork/timeline",
  "author": {
    "name": "azder",
    "email": "azhder@gmail.com"
  },
  "license": "MIT",
  "keywords": [],
  "scripts": {
    "esl:once": "eslint --ignore-path .gitignore ./",
    "esl:wtch": "nodemon --exec eslint --ignore-path .gitignore ./",
    "esl:ofix": "eslint --ignore-path .gitignore --fix ./",
    "esl:wfix": "nodemon --exec eslint --ignore-path .gitignore --fix ./",
    "smp:once": "eslint -c index.js ./doc/samples",
    "smp:wtch": "nodemon --exec npm run smp:once",
    "srv:covr": "npx http-server ./coverage/lcov-report/ -c-1 -p 0 ",
    "srv:bild": "npx http-server ./build/ -c-1 -p 0 ",
    "ncu:chck": "npx npm-check-updates",
    "ncu:cdoc": "npx npm-check-updates --doctor",
    "ncu:runu": "npx npm-check-updates -u",
    "ncu:rund": "npx npm-check-updates -u --doctor",
    "ncu:runm": "npx npm-check-updates -u --target minor",
    "ncu:runp": "npx npm-check-updates -u --target patch",
    "npm:deps": "npm i --legacy-peer-deps",
    "npm:lock": "npm i --legacy-peer-deps --package-lock-only",
    "npm:afix": "npm audit fix --legacy-peer-deps",
    "npm:ffix": "npm audit fix --force --legacy-peer-deps",
    "cra:start": "react-scripts start",
    "cra:build": "react-scripts build",
    "cra:test": "react-scripts test",
    "cra:eject": "react-scripts eject",
    "cro:start": "craco start",
    "cro:build": "craco build",
    "tst:once": "craco test --watchAll=false",
    "tst:ocvr": "craco test --watchAll=false--coverage",
    "tst:wtch": "craco test --watch",
    "tst:wcvr": "craco test --watchAll --coverage",
    "prepare": "husky install",
    "build": "npm run cro:build",
    "test": "npm run tst:once",
    "start": "npm run cro:start"
  },
  "cracoConfig": "craco.config.cjs",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "devDependencies": {
    "@craco/craco": "^6.4.3",
    "@jest/globals": "^27.4.6",
    "@me5on/albega": "^0.0.2",
    "@me5on/dv": "^0.0.3",
    "@sloy/eslint-config-import": "^0.0.1",
    "@sloy/eslint-config-recommended": "^0.1.1",
    "@sloy/jest-setup": "^0.0.1",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "eslint": "^8.6.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "http-server": "^14.0.0",
    "husky": "^7.0.4",
    "jest": "^27.4.7",
    "nodemon": "^2.0.15",
    "npm-check-updates": "^12.1.0",
    "react-scripts": "^5.0.0",
    "sass": "^1.45.2",
    "web-vitals": "^2.1.2"
  },
  "dependencies": {
    "@me5on/classicist": "^0.0.2",
    "@me5on/ob": "^0.1.0",
    "hoist-non-react-statics": "^3.3.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}